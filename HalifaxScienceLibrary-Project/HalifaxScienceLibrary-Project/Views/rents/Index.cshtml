@model IEnumerable<HalifaxScienceLibrary_Project.rent>

@{
    ViewBag.Title = "Index";
}

<h2>Rent Console</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cost_rent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cost_rent)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.rent_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.rent_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.rent_id })
        </td>
    </tr>
}

</table>
