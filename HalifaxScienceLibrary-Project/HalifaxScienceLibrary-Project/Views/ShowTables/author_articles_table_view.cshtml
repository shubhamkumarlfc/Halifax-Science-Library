
@model IEnumerable<HalifaxScienceLibrary_Project.author_articles>


@{
    ViewBag.Title = "author_article_table_view";
}

<h2>author_article_table_view</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.article.article_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.author.C_id)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.article.article_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.author.C_id)
            </td>

        </tr>
    }

</table>


<div>
    @Html.ActionLink("Back to List", "DisplayTables")
</div>