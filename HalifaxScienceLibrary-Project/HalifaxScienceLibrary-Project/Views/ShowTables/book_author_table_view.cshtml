
@model IEnumerable<HalifaxScienceLibrary_Project.book_author>


@{
    ViewBag.Title = "book_authors_table_view";
}

<h2>book_authors_table_view</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.author.lname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.book.name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.author.lname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.book.name)
            </td>

        </tr>
    }

</table>

<div>
    @Html.ActionLink("Back to List", "DisplayTables")
</div>