

@model IEnumerable<HalifaxScienceLibrary_Project.mothlyexpense_employee>

@{
    ViewBag.Title = "mothlyexpense_employee_table_view";
}

<h2>mothlyexpense_employee_table_view</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.hours_worked)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.employee.fname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.monthly_expense.year)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.hours_worked)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.employee.fname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.monthly_expense.year)
            </td>
          
        </tr>
    }

</table>

<div>
    @Html.ActionLink("Back to List", "DisplayTables")
</div>