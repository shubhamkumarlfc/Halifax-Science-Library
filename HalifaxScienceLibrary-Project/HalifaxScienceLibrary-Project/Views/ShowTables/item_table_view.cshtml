

@model IEnumerable<HalifaxScienceLibrary_Project.item>

@{
    ViewBag.Title = "item_table_view";
}

<h2>item_table_view</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.C_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.C_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.price)
        </td>

    </tr>
    }

</table>

<div>
    @Html.ActionLink("Back to List", "DisplayTables")
</div>