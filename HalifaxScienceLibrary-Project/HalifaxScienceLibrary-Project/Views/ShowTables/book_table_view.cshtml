
@model IEnumerable<HalifaxScienceLibrary_Project.book>

@{
    ViewBag.Title = "book_table_view";
}

<h2>book_table_view</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.item_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.item_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>

        </tr>
    }

</table>


<div>
    @Html.ActionLink("Back to List", "DisplayTables")
</div>